@model IEnumerable<OnlineJewellShop.Entity.User>

@{
    ViewBag.Title = "ViewProduct";
    
}

<h2>User's profile</h2>

<table >
    <tr>
        <th>UserId</th>
        <th>Mail</th>
        <th>Phone number</th> 
        <th>Role</th>
        <th>Update</th>
    </tr>

    @foreach (OnlineJewellShop.Entity.User user in Model)
    {
        <tr>
            <td>
                @user.UserID
            </td>

            <td>
                @user.MailId
            </td>
            <td>
                @user.PhoneNumber
            </td>
            <td>
                @user.Role
            </td>
            <td>
                @Html.ActionLink("Edit", "EditUser", new { id = user.UserID, @class = "LinkColor" })
                @Html.ActionLink("Delete", "DeleteUser", new { id = user.UserID, @class = "LinkColor" })
            </td>
        </tr>


    }
</table>
